{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "QL.Actions/*.csproj",
            "QL.FieldTransforms/*.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "../docs",
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appName": "qlsh",
      "_appTitle": "qlsh",
      "_enableSearch": true,
      "pdf": true
    }
  }
}